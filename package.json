{
	"name": "@sachinraja/eslint-config",
	"version": "0.3.0",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "index-wrapper.cjs",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"default": "./index-wrapper.cjs"
		}
	},
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"index-wrapper.cjs"
	],
	"scripts": {
		"build": "tsup",
		"format": "dprint fmt",
		"lint": "run-p lint:*",
		"lint:format": "dprint check",
		"lint:types": "tsc",
		"lint:js": "eslint .",
		"prepublishOnly": "pnpm build"
	},
	"peerDependencies": {
		"@types/eslint": "^8",
		"eslint": "^8 || ^9.0.0"
	},
	"peerDependenciesMeta": {
		"@types/eslint": {
			"optional": true
		}
	},
	"dependencies": {
		"@typescript-eslint/eslint-plugin": "6.2.1",
		"@typescript-eslint/parser": "6.2.1",
		"eslint-config-prettier": "8.10.0",
		"eslint-plugin-unicorn": "48.0.1"
	},
	"devDependencies": {
		"@types/eslint": "8.44.1",
		"@types/node": "20.4.6",
		"dprint": "0.40.2",
		"eslint": "8.46.0",
		"npm-run-all": "4.1.5",
		"tsup": "7.2.0",
		"typescript": "5.1.6"
	},
	"sideEffects": false,
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"format": [
			"cjs"
		],
		"dts": {
			"resolve": true
		},
		"splitting": true,
		"clean": true
	},
	"packageManager": "pnpm@8.5.1"
}
